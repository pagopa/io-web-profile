{
  "name": "io-web-test",
  "version": "0.10.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "copy .env .env.local && next build",
    "build:macos": "cp .env .env.local && next build",
    "build:prod": "next build && cp -f out/it/404/index.* out/404/ && cp -f out/it/404/index.html out/404.html && cp -f out/it/404/index.txt out/404.txt",
    "start": "next start",
    "start-static": "npx serve@latest out",
    "test": "jest",
    "lint": "next lint",
    "prepare": "husky install",
    "generate": "npm-run-all generate:*",
    "clean:api-web-profile": "rimraf src/api/generated/webProfile",
    "generate:api-web-profile": "yarn run clean:api-web-profile && shx mkdir -p src/api/generated/webProfile && gen-api-models --api-spec ./openApi/openapi.yaml --request-types --response-decoders  --no-strict --client --out-dir src/api/generated/webProfile"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/lab": "^5.0.0-alpha.142",
    "@mui/material": "^5.14.5",
    "@mui/system": "^5.14.5",
    "@pagopa/mui-italia": "^1.0.1",
    "@pagopa/pagopa-editorial-components": "^2.1.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.2",
    "@types/mixpanel-browser": "^2.47.0",
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "cookies-next": "^2.1.2",
    "crypto-js": "^4.1.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "generate-password-ts": "^1.6.3",
    "mixpanel-browser": "^2.47.0",
    "next": "13.4.9",
    "next-intl": "2.19.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-md-spinner": "^1.0.0",
    "react-redux": "^8.1.1",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@pagopa/eslint-config": "^3.0.0",
    "@pagopa/openapi-codegen-ts": "^13.1.0",
    "encoding": "^0.1.13",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4"
  }
}
